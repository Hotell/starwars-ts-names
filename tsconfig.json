{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "outDir": "lib",
    "declaration": true,
    "declarationDir": "typings",
    "pretty": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
